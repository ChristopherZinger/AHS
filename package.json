{
	"name": "architecturehorrorstory",
	"version": "0.0.1",
	"private": "true",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"local:up": "docker compose -f docker-compose.local.yml up -d",
		"local:down": "docker compose -f docker-compose.local.yml down",
		"dev:images": "docker compose -f docker-compose.dev.yml up -d",
		"dev:images:down": "docker compose -f docker-compose.dev.yml down",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"docker:start": "docker compose start",
		"docker:stop": "docker compose stop ",
		"pmdn": "npx prisma migrate dev --name",
		"prisma-gen": "npx prisma generate",
		"prisma:migrate": "npx prisma migrate dev --name",
		"prisma:push": "npx prisma db push",
		"serve-production": "node -r dotenv/config index.js"
	},
	"dependencies": {
		"@prisma/client": "^4.8.1",
		"bcrypt": "^5.1.1",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"jsonwebtoken": "^9.0.1",
		"lodash": "^4.17.21",
		"svelte-preprocess": "^5.0.0",
		"svelte-select": "^5.3.1",
		"yup": "^0.32.11",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.1.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/lodash": "^4.14.191",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"nanoid": "^4.0.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.8.1",
		"slugify": "^1.6.5",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.2.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
